<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="user" init-method="init" destroy-method="destroy"
		class="com.softtron.spring.domains.后处理器BeanPostProcessor.User"></bean>
	<bean id="postProcessor" 
		class="com.softtron.spring.domains.后处理器BeanPostProcessor.PostProcessor"></bean>
</beans>